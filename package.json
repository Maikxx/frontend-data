{
  "name": "frontend-data",
  "description": "A repository for the course frontend-data.",
  "private": true,
  "scripts": {
    "serve": "nodemon",
    "lint": "tslint index.ts",
    "start:client": "yarn clearDist && parcel index.dev.html",
    "build:client": "yarn clearDist && parcel build index.dev.html --no-source-maps && mv dist/index.dev.html ./index.html && code ./index.html",
    "clearDist": "rm -rf dist/"
  },
  "version": "1.0.0",
  "main": "index.ts",
  "repository": "git@github.com:Maikxx/frontend-data.git",
  "author": "Maikel van Veen <maikeloo10@hotmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/polyfill": "^7.0.0",
    "@types/d3": "^5.0.1",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/lodash.chunk": "^4.2.4",
    "@types/lodash.flatten": "^4.4.4",
    "@types/node-fetch": "^2.1.3",
    "autoprefixer": "^9.3.1",
    "nodemon": "^1.18.6",
    "parcel-bundler": "^1.10.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "@turf/along": "^6.0.1",
    "@turf/length": "^6.0.2",
    "babel-polyfill": "^6.26.0",
    "d3": "^5.7.0",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "lodash.chunk": "^4.2.0",
    "lodash.flatten": "^4.4.0",
    "node-fetch": "^2.3.0",
    "node-oba-api-wrapper": "maanlamp/node-oba-api-wrapper"
  }
}
